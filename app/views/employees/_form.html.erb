<%= form_for @employee do |t| %>
  <%= t.label :full_name %>
  <%= t.text_field :full_name %>

  <%= t.label :email %>
  <%= t.text_field :email %>

  <%= t.label :address %>
  <%= t.text_field :address %>

  <%= t.label :password %>
  <%= t.password_field :password %>
 
  <%= t.submit 'Save' %>
<% end %>