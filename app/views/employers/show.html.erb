<% if logged_in? && session[:employer_id] == @employer.id %>
 <%= link_to 'Log out', logout_path, method: :delete %>
  <% @applicantions.each do |t, v, e| %>
    <p><%= t.title %> <%= e.email %></p>
    <% if v == false %>
      <%= button_to "approve", approve_path(t.id) %>
    <% else %>
      <%= button_to "disapprove", disapprove_path(t.id) %>
    <% end %>
  <% end %>
<% end %>
